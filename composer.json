{
	"name": "lw/l10n-translator",
	"type": "typo3-cms-extension",
	"description": "Translate l10n files (label files) in a TYPO3 backend module. Maintain l10n folder with CLI tasks.",
	"authors": [
		{
			"name": "Achim Fritz",
			"role": "Developer"
		},
		{
			"name": "Daniel Goerz",
			"role": "Developer"
		}
	],
	"license": ["GPL-2.0-or-later"],
	"require": {
		"typo3/cms-core": "^9.5"
	},
	"replace": {
		"l10n_translator": "self.version",
		"typo3-ter/l10n-translator": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Lightwerk\\L10nTranslator\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Lightwerk\\L10nTranslator\\Tests\\": "Tests"
		}
	}
}
